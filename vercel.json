{
    "builds": [
        {
            "src": "server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/dist/**",
            "use": "@vercel/static"
        },
        {
            "src": "sally.jpg",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.js"
        },
        {
            "src": "/sally.jpg",
            "dest": "/sally.jpg"
        },
        {
            "src": "/(.*)",
            "dest": "/client/dist/$1"
        }
    ]
}